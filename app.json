{
  "expo": {
    "name": "MotorFi",
    "slug": "motorfi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Stackathon Project for FullStack Academy. MotorFi is a react-native app that lets users browse a selection of high end automobiles, as well as view technical specs per car and availability on forums.",
    "githubUrl": "https://github.com/ScottGetz/stackathon"
  }
}
